{
  "name": "maja-umred",
  "version": "1.0.0",
  "description": "National-Scale E-Governance Initiative for Urban India - Real-time ward-wise performance dashboard of civic services",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon server/index.js",
    "dev:frontend": "next dev",
    "build": "next build",
    "build:backend": "tsc --build",
    "start": "node server/index.js",
    "start:frontend": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "cypress run",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "migrate": "npm run db:migrate",
    "prepare": "husky install",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "keywords": [
    "e-governance",
    "smart-cities",
    "civic-services",
    "urban-india",
    "government",
    "complaint-management",
    "ward-monitoring",
    "public-accountability",
    "digital-india",
    "swachh-bharat",
    "amrut",
    "citizen-services",
    "transparency",
    "governance"
